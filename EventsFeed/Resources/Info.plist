<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>CFBundleURLTypes</key>
    <array>
        <!-- Google Sign-In (існуючий) -->
        <dict>
            <key>CFBundleTypeRole</key>
            <string>Editor</string>
            <key>CFBundleURLName</key>
            <string>Google Sign-In</string>
            <key>CFBundleURLSchemes</key>
            <array>
                <string>com.googleusercontent.apps.487751122186-an2bm47rbc80taujsrjgfqv3ta4rlumn</string>
            </array>
        </dict>
        <!-- Spotify Auth -->
        <dict>
            <key>CFBundleTypeRole</key>
            <string>Editor</string>
            <key>CFBundleURLName</key>
            <string>Spotify Auth</string>
            <key>CFBundleURLSchemes</key>
            <array>
                <string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
            </array>
        </dict>
        <!-- YouTube Music Auth (оновлений) -->
        <dict>
            <key>CFBundleTypeRole</key>
            <string>Editor</string>
            <key>CFBundleURLName</key>
            <string>YouTube Music Auth</string>
            <key>CFBundleURLSchemes</key>
            <array>
                <string>com.googleusercontent.apps.487751122186-3jkc1mkdi0pfr82kcpjtnipv470utl1s</string>
            </array>
        </dict>
    </array>
    
    <!-- Queries Schemes -->
    <key>LSApplicationQueriesSchemes</key>
    <array>
        <string>spotify</string>
        <string>youtubemusic</string>
    </array>
    
    <!-- App Transport Security -->
    <key>NSAppTransportSecurity</key>
    <dict>
        <key>NSAllowsArbitraryLoads</key>
        <true/>
    </dict>
</dict>
</plist>
